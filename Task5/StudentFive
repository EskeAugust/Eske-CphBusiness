public class Student
{
	public String name;
	public int age;
	public String isFemale;
	public int datamatikerTeam;

	public Student(String tmpName, int tmpAge, String tmpIsFemale, int tmpDatamatikerTeam)
	{
		this.name = tmpName;
		this.age = tmpAge;
		this.isFemale = tmpIsFemale;
		this.datamatikerTeam = tmpDatamatikerTeam;
	}
}